<Window x:Class="LoginUsingBCrypt.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LoginUsingBCrypt"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox Name="EmailTxt" HorizontalAlignment="Left" Height="23" Margin="215,105,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="PasswordTxt" HorizontalAlignment="Left" Height="23" Margin="215,146,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button Name="LoginBtn" Content="Login" HorizontalAlignment="Left" Margin="368,105,0,0" VerticalAlignment="Top" Width="75" Click="LoginBtn_Click"/>
        <Button Name="RegisterBtn" Content="Register" HorizontalAlignment="Left" Margin="368,149,0,0" VerticalAlignment="Top" Width="75" Click="RegisterBtn_Click"/>
    </Grid>
</Window>
